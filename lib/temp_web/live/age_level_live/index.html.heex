<h1>Listing Agelevels</h1>

<%= if @live_action in [:new, :edit] do %>
  <.modal return_to={Routes.age_level_index_path(@socket, :index)}>
    <.live_component
      module={TempWeb.AgeLevelLive.FormComponent}
      id={@age_level.id || :new}
      title={@page_title}
      action={@live_action}
      age_level={@age_level}
      return_to={Routes.age_level_index_path(@socket, :index)}
    />
  </.modal>
<% end %>

<table>
  <thead>
    <tr>
      <th>Agelevel</th>

      <th></th>
    </tr>
  </thead>
  <tbody id="age_levels">
    <%= for age_level <- @age_levels do %>
      <tr id={"age_level-#{age_level.id}"}>
        <td><%= age_level.age_level %></td>

        <td>
          <span><%= live_redirect "Show", to: Routes.age_level_show_path(@socket, :show, age_level) %></span>
          <span><%= live_patch "Edit", to: Routes.age_level_index_path(@socket, :edit, age_level) %></span>
          <span><%= link "Delete", to: "#", phx_click: "delete", phx_value_id: age_level.id, data: [confirm: "Are you sure?"] %></span>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<span><%= live_patch "New Age level", to: Routes.age_level_index_path(@socket, :new) %></span>
